<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" fx:id="geometryTabPane" tabClosingPolicy="UNAVAILABLE"
         fx:controller="cz.vutbr.fit.pdb.component.rightbar.geometry.circleinfo.CircleInfoPresenter">
    <tabs>
        <Tab fx:id="circleTab" text="Circle">
            <content>
                <VBox fx:id="vbox">
                    <GridPane>
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <children>
                            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CenterX"
                                  GridPane.halignment="CENTER"/>
                            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CenterY"
                                  GridPane.halignment="CENTER" GridPane.rowIndex="1"/>
                            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Radius"
                                  GridPane.halignment="CENTER" GridPane.rowIndex="2"/>
                            <TextField fx:id="centerXField" GridPane.columnIndex="1"/>
                            <TextField fx:id="centerYField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            <TextField fx:id="radiusField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                        </children>
                    </GridPane>
                </VBox>
            </content>
        </Tab>
    </tabs>
</TabPane>